{
  "kind": "collectionType",
  "collectionName": "faqs",
  "info": {
    "singularName": "faq",
    "pluralName": "faqs",
    "displayName": "Faq"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "question": {
      "type": "string",
      "required": true,
      "unique": true,
      "minLength": 1
    },
    "answer": {
      "type": "richtext",
      "required": true
    },
    "tags": {
      "type": "enumeration",
      "enum": [
        "courses",
        "pathways",
        "providers"
      ],
      "required": true,
      "default": "courses"
    }
  }
}
